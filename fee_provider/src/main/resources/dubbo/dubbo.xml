<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    	http://www.springframework.org/schema/beans/spring-beans.xsd
		http://code.alibabatech.com/schema/dubbo
		http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<dubbo:provider connections="10"/>
	
    <dubbo:application name="${zkServiceAppName}" owner="kylin.tong" organization="FlightRoutes-Flight"/>

    <dubbo:protocol name="dubbo" port="${dubbo.port}" threads="200"/>

    <import resource="dubbo-expose.xml"/>
</beans>