<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    	http://www.springframework.org/schema/beans/spring-beans.xsd
		http://code.alibabatech.com/schema/dubbo
		http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


    <dubbo:registry protocol="zookeeper" address="${zkAdds}" group="${dubbo.account.group}" id="fr_f_account"/>

    <dubbo:reference
            id="purchaserClassificationApiFacade"
            interface="com.flightroutes.flight.account.facade.apiService.PurchaserClassificationApiFacade"
            version="1.0.0"
            cluster="failfast"
            timeout="50000"
            check="true"
            registry="fr_f_account">
    </dubbo:reference>

    <dubbo:reference
            id="supplierFacade"
            interface="com.flightroutes.flight.account.facade.ISupplierFacade"
            version="1.0.0"
            cluster="failfast"
            timeout="50000"
            check="true"
            registry="fr_f_account">
    </dubbo:reference>

</beans>